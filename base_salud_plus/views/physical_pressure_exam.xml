<odoo>
    <record id="physical_pressure_exam_view_tree" model="ir.ui.view">
        <field name="name">physical_pressure_exam_view_tree</field>
        <field name="model">physical.pressure.exam</field>
        <field name="arch" type="xml">
            <tree string="Examenes" editable="top">
                <field name="name"/>
                <field name="uom_id"/>
            </tree>
        </field>
    </record>

    <record id="physical_pressure_exam_action" model="ir.actions.act_window">
        <field name="name">Examenes</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">physical.pressure.exam</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>